<?php

if (!empty($_SERVER['argv'][0])){
    if ($_SERVER['argv'][0] === 'sk') {
        array_splice($_SERVER['argv'], 0, 1);
    }
    if (!empty($_SERVER['argv'][0])){
        $_SERVER['REQUEST_URI'] = $_SERVER['argv'][0];
        array_splice($_SERVER['argv'], 0, 1);
    }
}

include "skWeb.php";
